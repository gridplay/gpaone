{
    "name": "gridplay/gpaone",
    "description": "GridPlay OAuth2 Provider for Laravel Socialite",
    "license": "MIT",
    "keywords": [
        "gridplay",
        "laravel",
        "oauth",
        "provider",
        "socialite",
        "gpaone"
    ],
    "authors": [
        {
            "name": "Christopher Strachan",
            "email": "chrisx84@live.ca"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "socialiteproviders/manager": "^4.5",
        "guzzlehttp/guzzle": "7.*",
        "illuminate/support": "11.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GPa\\GPaServiceProvider"
            ],
            "aliases": {
                "GPa": "GPa\\GPa"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "GPAONE\\": "",
            "GPa\\": "src"
        }
    }
}
